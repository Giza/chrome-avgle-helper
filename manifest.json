{
	"manifest_version": 2,
	"name": "Avgle Helper",
	"version": "1.1.0",
	"description": "A Chrome extension for enhancing Avgle website",
	"background": {
		"scripts": ["dist/index.js"],
		"persistent": true
	},
	"icons": {
		"128": "icons/128.png"
	},
	"permissions": [
		"tabs",
		"notifications",
		"webRequest",
		"*://*/*"
	]
}
